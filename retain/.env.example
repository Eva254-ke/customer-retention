# Sample environment variables for Retain project

# Django settings
DJANGO_SECRET_KEY=your_secret_key_here
DJANGO_DEBUG=True
DJANGO_ALLOWED_HOSTS=localhost,127.0.0.1

# Database settings
DATABASE_NAME=your_database_name
DATABASE_USER=your_database_user
DATABASE_PASSWORD=your_database_password
DATABASE_HOST=localhost
DATABASE_PORT=5432

# Event Analytics Service
EVENT_ANALYTICS_DB_URL=your_event_analytics_db_url

# AI Inference Service
GROQ_API_KEY=your_groq_api_key_here

# Communication Service
AFRICAS_TALKING_API_KEY=your_africas_talking_api_key_here
AFRICAS_TALKING_USERNAME=your_africas_talking_username_here

# Celery settings
CELERY_BROKER_URL=redis://localhost:6379/0

# Monitoring settings
PROMETHEUS_URL=http://prometheus:9090

# Other settings
ENVIRONMENT=development